#!/bin/python3

import math
import os
import random
import re
import sys

def extraLongFactorials(n):
    if n>1 : return n*extraLongFactorials(n-1)
    else:return 1


if __name__ == '__main__':
    n = int(input())

    re=extraLongFactorials(n)
    print(re)
